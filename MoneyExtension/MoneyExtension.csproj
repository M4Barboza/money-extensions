<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net9.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>

    <PackageId>Teste123MoneyExtension</PackageId>
    <Version>1.0.0</Version>
    <License>MIT</License>
    <Authors>Matheus Barboza dos Santos</Authors>
    <PackageDescription>Money extension for C#</PackageDescription>
    <RepositoryUrl>https://github.com/M4Barboza/money-extensions</RepositoryUrl>
    <PackageProjectUrl>https://github.com/M4Barboza/money-extensions</PackageProjectUrl>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageIcon>icon.png</PackageIcon>
    <PackageTags>money, extension, csharp</PackageTags>
    <Title>MoneyExtension</Title>
    <Summary>Money extension for C#</Summary>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\Media\icon.png" Pack="true" PackagePath="\"/>
  </ItemGroup>

</Project>
